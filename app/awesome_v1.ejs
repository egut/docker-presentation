<div class="row">
  <div class="col s12">
    <div id="color-usage" class="section scrollspy">
      <h2 class="header">Awesome WebApps v1 - <i>in app, docker</i></h2>
    </div>
  </div>
</div>

<div class="row">
  <div id="docker" class="col s6 ">
    <div class="row  col s12 box z-depth-2 valign-wrapper">
      <div class="valign center-align">

        <div class="row">
          <div class="col s12">
            <h5 class="header" >Docker connect</h5>
          </div>
        </div>

        <div class="input-field col s12">
          <input name="docker_host" id="docker_host" type="text" value="localhost:3000">
          <label for="docker_host">Enter hostname</label>
        </div>

        <div class="input-field col s12">
          <input name="docker_path" id="docker_path" type="text" value="/awesome/v1/app">
          <label for="docker_path">Path on host</label>
        </div>

        <div class="col s12">
          <button class="waves-effect waves-light btn deep-orange lighten-1" onclick="return docker_connect($('#docker_host').val(),$('#docker_path').val(),'docker');">Connect</button>
        </div>
      </div>
    </div>
  </div>


