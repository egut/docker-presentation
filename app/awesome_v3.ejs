            </div>
          </div>

          <div class="input-field col s12">
            <input name="docker_host2" id="docker_host2" type="text" value="localhost:3000">
            <label for="docker_host2">Enter hostname</label>
          </div>

          <div class="input-field col s12">
            <input name="docker_path2" id="docker_path2" type="text" value="/awesome/v3/app">
            <label for="docker_path2">Path on host</label>
          </div>

          <div class="col s12">
            <button class="waves-effect waves-light btn deep-orange lighten-1" onclick="return docker_connect($('#docker_host2').val(),$('#docker_path2').val(),'docker2');">Connect</button>
          </div>
        </div>
      </div>
    </div>
